{
  "name": "behavior-driven-ui-webdriver",
  "version": "0.1.0",
  "description": "WebDriver driver implementation for behavior-driven-ui",
  "keywords": [
    "testing",
    "bdd",
    "ui-testing",
    "webdriver",
    "selenium",
    "behavior-driven",
    "e2e"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm run type:check && tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "type:check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "selenium-webdriver": "^4.15.0"
  },
  "devDependencies": {
    "@types/selenium-webdriver": "^4.1.0",
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "behavior-driven-ui": "workspace:^"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  },
  "publishConfig": {
    "access": "public"
  }
}